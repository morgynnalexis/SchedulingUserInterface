<AnchorPane id="AnchorPane" prefHeight="203.0" prefWidth="336.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="schedulinguserinterface.SelectViewController">
   <children>
      <BorderPane prefHeight="200.0" prefWidth="350.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <center>
            <GridPane BorderPane.alignment="CENTER">
              <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
              </columnConstraints>
              <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
              </rowConstraints>
               <children>
                  <Button fx:id="generateReportsBttn" mnemonicParsing="false" onAction="#viewReports" prefHeight="30.0" prefWidth="160.0" text="Generate Report" />
                  <Button fx:id="viewApptsBttn" mnemonicParsing="false" onAction="#viewAppts" prefHeight="30.0" prefWidth="160.0" text="View Appointments" GridPane.columnIndex="1" />
                  <Button fx:id="viewCustBttn" mnemonicParsing="false" onAction="#ViewCust" prefHeight="30.0" prefWidth="160.0" text="View Customers" GridPane.rowIndex="1" />
                  <Button fx:id="viewCalBttn" mnemonicParsing="false" onAction="#ViewCal" prefHeight="30.0" prefWidth="160.0" text="View Calendar" GridPane.columnIndex="1" GridPane.rowIndex="1" />
               </children>
            </GridPane>
         </center>
         <bottom>
            <Button fx:id="logoutBttn" mnemonicParsing="false" onAction="#logout" prefHeight="30.0" prefWidth="130.0" text="Log out" BorderPane.alignment="CENTER" />
         </bottom>
         <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </padding>
      </BorderPane>
   </children>
   <padding>
      <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
   </padding>
</AnchorPane>
